{
  "authors": [
    {
      "email": "drupal@pnnl.gov",
      "name": "PNNL Drupal Development Team"
    }
  ],
  "autoload": {
    "psr-4": {
      "pnnl\\Composer\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "pnnl\\Tests\\": "tests"
    }
  },
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "7.3"
    }
  },
  "conflict": {
    "drupal/drupal": "7.*"
  },
  "description": "Composer installer for Drupal modules, themes, etc",
  "extra": {
    "class": "pnnl\\Composer\\DrupalInstallerPlugin"
  },
  "license": "MIT",
  "minimum-stability": "dev",
  "name": "pnnl/drupal-installer",
  "prefer-stable": true,
  "require": {
    "composer-plugin-api": "^2.0"
  },
  "require-dev": {
    "enlightn/security-checker": "^1.9",
    "nikic/php-parser": "^3.1 || ^4.2",
    "php-coveralls/php-coveralls": "^2.0",
    "php-parallel-lint/php-parallel-lint": "^1.3",
    "phpmd/phpmd": "^2.6",
    "phpro/grumphp": "^1.5",
    "phpunit/phpunit": "^9.0",
    "squizlabs/php_codesniffer": "^3.4"
  },
  "scripts": {
    "test": "phpunit"
  },
  "type": "composer-plugin"
}
